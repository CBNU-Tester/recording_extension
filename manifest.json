{
    "name": "ChromeExtension",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "scripting",
      "identity",
      "identity.email",
      "alarms",
      "storage",
      "activeTab",
      "notifications",
      "tabs"
    ],
    "host_permissions": [
      "https://www.google.com/*",
      "https://www.cbnutester.site/*"
    ],
    
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
          "16": "images/icon16.png"
        }
  }
  }